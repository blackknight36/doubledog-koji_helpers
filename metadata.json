{
    "name": "doubledog-koji_helpers",
    "version": "1.3.0",
    "author": "John Florian",
    "license": "GPL-3.0",
    "summary": "A module to manage supplementary tools to help in a Koji deployment",
    "source": "https://github.com/jflorian/doubledog-koji_helpers",
    "project_page": "https://github.com/jflorian/doubledog-koji_helpers",
    "issues_url": "https://github.com/jflorian/doubledog-koji_helpers/issues",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemmajrelease": [
                "7"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "doubledog/ddolib",
            "version_requirement": ">= 1.0.0 < 2.0.0"
        },
        {
            "name": "puppetlabs/concat",
            "version_requirement": ">= 4.0.0 < 6.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 3.2.0 < 6.0.0"
        }
    ]
}
